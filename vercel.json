//     { "src": "/(.*)", "dest": "/index.js" }
//   ]
// }


{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/upload", "dest": "/upload" },
    { "src": "/data", "dest": "/data" },
    { "src": "/delete/(.*)", "dest": "/delete/$1" }
  ]
}
